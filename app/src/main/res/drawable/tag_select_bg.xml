<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 设置按钮按下去的效果，实际上就是不同的图片
     selector：根节点，其下可有多个item标签，智慧选择符合的item显示
     corners:   设置圆角
     state_pressed：true，被按下
     stroke : 设置边框
     solid : 设置背景色
     -->

    <item android:state_pressed="false" >
        <shape>
            <corners android:radius="90dp"/>
            <stroke android:color="@color/colorLightBlue8" android:width="1dp"/>
            <solid android:color="@color/colorLightBlue8"/>
        </shape>
    </item>


</selector>
