<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 设置按钮按下去的效果，实际上就是不同的图片
     selector：根节点，其下可有多个item标签，智慧选择符合的item显示
      corners radius 设置四个角的角度，也可使用其他属性分开设置四个角的角度
     state_pressed：true，被按下
     -->

    <item android:state_pressed="false" >
        <shape>
            <corners android:radius="90dp"/>
            <solid android:color="@color/colorLightBlue8"/>
        </shape>
    </item>


</selector>
